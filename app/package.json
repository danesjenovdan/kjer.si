  "scripts": {
    "release:android": "NODE_OPTIONS=--max-old-space-size=4096 tns build android --release --key-store-path=./kjersi.keystore --key-store-password=$PASSWORD --key-store-alias=kjersi --key-store-alias-password=$PASSWORD --env.snapshot --bundle --env.aot --env.uglify --env.environment=prod --aab && open platforms/android/app/build/outputs/",
    "release:android:apk": "NODE_OPTIONS=--max-old-space-size=4096 tns build android --release --key-store-path=./kjersi.keystore --key-store-password=$PASSWORD --key-store-alias=kjersi --key-store-alias-password=$PASSWORD --env.snapshot --bundle --env.aot --env.uglify --env.environment=prod && open platforms/android/app/build/outputs/apk"
  },
