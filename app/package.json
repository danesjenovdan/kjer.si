    "axios": "^0.19.0",
    "nativescript-geolocation": "^5.1.0",
    "nativescript-google-maps-sdk": "^2.8.1",
    "nativescript-localize": "^4.2.0",
    "nativescript-localstorage": "^2.0.1",
    "nativescript-lottie": "3.0.3",
